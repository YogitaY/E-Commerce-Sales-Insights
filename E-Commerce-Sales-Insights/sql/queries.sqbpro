ql name="SQL 1*">--Total Sales
select sum(Sales) As Total_Sales from sales;

--Total Profit
Select sum(Sales) As Total_Profit from sales;

--Sales by Region
Select Region, sum(sales) As Total_Sale 
from sales 
Group by Region 
Order by Total_Sale Desc;

--Top 5 Customers by Sales
select Customer_Name, sum(sales) As Customer_Sales
from sales
Group by Customer_Name
Order by Customer_Sales Desc
LIMIT 5;

--Top 10 Products by Total Sales
select Product_Name, sum(sales) As Total_Sales
from sales
group by Product_Name
order by Total_Sales DESC
Limit 10;	

--Top 10 Products by Profit
select Product_Name, sum(profit) As Total_Profit
from sales
group by Product_Name
order by Total_Profit DESC
Limit 10;

--Most Frequently Ordered Products
select Product_Name, count(*) As Order_Count
from sales	
group by Product_Name
order by Order_Count Desc
Limit 10;

--Average Profit Margin by Product
select Product_Name, round(sum(profit)/sum(sales)*100,2) As Profit_Margin_Percentage
from sales
Group by Product_Name
HAVING sum(sales) &gt; 0
order by Profit_Margin_Percentage DESC
LIMIT 10;

--Product Performance by Region
select Region, Product_Name, sum(sales) As Total_Sales
from sales
group by Region, Product_Name
ORDER by Region, Total_Sales DESC;

--Compare Categories
Select Category, sum(sales) As Total_Sales
from sales
Group by Category
order by Total_Sales Desc;

--Identify Low-Profit Products
select Product_Name, round(profit/sales *100,2) As Low_Profit
from sales
WHERE sales&gt;0
order by Low_Profit ASC;

/*1. Product Performance by Region
Goal: See which products sell best in each region.*/

SELECT Region, Product_Name, sum(sales) As Total_Sales
from sales
Group by Region,Product_Name
order by Region,Total_Sales desc;

/*2. Compare Categories
Goal: Find which product categories bring in the most revenue and profit.*/

SELECT Category,sum(sales) as Total_Sales, sum(profit) As Total_Profit
from sales
Group by Category
ORDER by Total_Profit Desc;

/*3. Identify Low-Profit Products to Avoid
Goal: Detect products with poor profit margins to reduce or remove.*/

Select Product_Name, round(sum(profit)/sum(sales)*100,2) as Profit_margin
from sales
GROUP by Product_Name
having sum(sales) &gt; 0
Order by Profit_margin ASC;

/*4. Customer Behavior Analysis
Goal: Identify the most valuable customers and their buying habits.*/

Select Customer_Name, count(*) As Orders, sum(sales) As Total_Sales
from sales
GROUP by Customer_Name
ORDER by Total_Sales Desc
LIMIT 10;

/*5. Monthly Sales &amp; Profit Trends
Goal: Understand performance over time (seasonality, growth, slowdowns).*/

SELECT substr(Order_Date,4,10) As Month, Sum(sales) as Total_Sales, sum(profit) as Total_Profit
from sales
Group by Month
order by Month;

/*6. Profit Margin by Segment
Goal: Discover which customer segments are the most profitable.*/

SELECT Segment,round(sum(profit)/sum(sales)*100,2) As Profit_Margin,Sum(Profit) as Total_Profit, sum(sales) As Total_Sales
from sales
Group by Segment
ORDER by Profit_Margin Desc;







































